{
    "name": "ntwklr/laravel-shariff",
    "description": "Laravel Wrapper for Shariff",
    "keywords": [
        "ntwklr",
        "laravel",
        "shariff",
        "laravel-shariff",
        "social"
    ],
    "homepage": "https://github.com/ntwklr/laravel-shariff",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael RÃ¶del",
            "email": "hello@mroedel.de",
            "homepage": "https://mroedel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0",
        "heise/shariff": "~7.1.3"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Ntwklr\\Shariff\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ntwklr\\Shariff\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ntwklr\\Shariff\\ShariffServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
